{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["Shopkart/server/**"]
      }
    },
    {
      "src": "Shopkart/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/Shopkart/client/build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/Shopkart/client/build/favicon.ico"
    },
    {
      "src": "/asset-manifest.json",
      "dest": "/Shopkart/client/build/asset-manifest.json"
    },
    {
      "src": "/manifest.json",
      "dest": "/Shopkart/client/build/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/Shopkart/client/build/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "/Shopkart/client/build/logo512.png"
    },
    {
      "src": "/(.*)",
      "dest": "/Shopkart/client/build/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

